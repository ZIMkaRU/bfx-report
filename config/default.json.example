{
  "app": {
    "port": 31339,
    "host": "127.0.0.1",
    "cors": {
      "enable": true
    },
    "redirectCsvElectron": {
      "enable": false,
      "url": "https://report.bitfinex.com",
      "methods": [
        "getTradesCsv",
        "getLedgersCsv",
        "getOrdersCsv",
        "getMovementsCsv"
      ]
    }
  },
  "auth": {
    "apiKey": "",
    "apiSecret": ""
  },
  "grenacheClient": {
    "query": "rest:report:api",
    "grape": "http://127.0.0.1:30001",
    "timeout": 10000
  },
  "grenacheService": {
    "restUrl": "https://api.bitfinex.com",
    "grape1": {
      "dht_port": 20002,
      "dht_bootstrap": ["127.0.0.1:20001"],
      "api_port": 40001
    },
    "grape2": {
      "dht_port": 20001,
      "dht_bootstrap": ["127.0.0.1:20002"],
      "api_port": 30001
    }
  },
  "logDebug": {
    "prefix": "",
    "postfix": ":app",
    "enableInfo": false,
    "enableDev": false,
    "enableError": false,
    "enableServerStream": false
  },
  "log": {
    "label": ":app",
    "pathError": null,
    "pathExcLogger": null,
    "pathLog": null,
    "enableConsole": false,
    "enableColor": true,
    "enableColorPathError": false,
    "enableColorPathExcLogger": false,
    "enableColorPathLog": false,
    "maxSize": null
  }
}
